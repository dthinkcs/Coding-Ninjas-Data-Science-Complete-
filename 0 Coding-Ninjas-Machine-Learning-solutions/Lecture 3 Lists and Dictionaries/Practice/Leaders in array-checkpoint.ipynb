      "6\n",
      "3 12 34 2 0 -1\n",
      "34 2 0 -1 "
     ]
    }
   ],
   "source": [
    "n=int(input())\n",
    "l=[int(n) for n in input().strip().split()]\n",
    "maximum=-1\n",
    "leader=[]\n",
    "for i in range(len(l)-1,-1,-1):\n",
    "    if maximum<=l[i]:\n",
    "        leader.append(l[i])\n",
    "        maximum=l[i]\n",
    "leader.reverse()\n",
    "for i in range(len(leader)):\n",
    "    print(leader[i],end=\" \")"
   ]
  }
